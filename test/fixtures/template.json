      }]
    }, {
      "type": "helper",
      "name": "Pass",
      "attrs": [{
        "name": "key",
        "value": "@index"
      }, {
        "name": "value",
        "value": "0"
      }],
      "children": [{
        "type": "tag",
        "tagName": "div",
        "children": [{
          "type": "textnode",
          "value": "Pass"
        }]
      }]
