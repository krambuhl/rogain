      "children": [{
        "type": "tag",
        "tagName": "div",
        "children": [{
          "type": "textnode",
          "data": [{
            "type": "variable",
            "path": "@attrs.key"
          }, {
            "type": "text",
            "value": " - "
          }, {
            "type": "variable",
            "path": "@attrs.value"
          }]
        }]
